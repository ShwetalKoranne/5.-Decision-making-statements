#include <iostream>
using namespace std;

int main() {
    while (1) {
        int choice;
        cout << "1. Name" << endl;
        cout << "2. Date of Birth" << endl;
        cout << "3. Address" << endl;
        cout << "4. College Name" << endl;
        cout << "5. Exit" << endl;

        cout << "Enter choice: ";
        cin >> choice;

        switch(choice) {
            case 1:
                cout << "Name: Shwetal";
                break;
            case 2:
                cout << "Date of Birth: 18-Jan-2006";
                break;
            case 3:
                cout << "Address: B-106, Main Street, Pune";
                break;
            case 4:
                cout << "College Name: Symbiosis Institute of College";
                break;
            case 5:
                cout << "Exiting...";
                return 0;
            default:
                cout << "Invalid choice" << endl;
        }

        cout << endl;
    }

    return 0;
}


